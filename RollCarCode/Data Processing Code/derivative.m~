function [deriv1,deriv2]=derivative(xData,yData)
    d=4;
    
    deriv1=zeros(size(xData));
    deriv2=zeros(size(yData));
    
    for m=1:length(xData)-d   
        p=polyfit(xData(m:m+d),yData(m:m+d),d);
        p_prime=polyder(p);
        p_prime_prime=polyder(p_prime);
        
        for n=1:d
            deriv1(n)=polyval(p_prime,xData(m
        end
    end
end